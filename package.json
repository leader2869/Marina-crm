{
  "name": "marina-crm",
  "version": "1.0.0",
  "description": "CRM система для управления яхт-клубами",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:client": "cd client && npm run dev",
    "build": "node -e \"process.exit(0)\"",
    "build:server": "tsc",
    "build:client": "cd client && npm run build",
    "start": "node dist/server.js",
    "migrate": "ts-node src/database/migrate.ts",
    "seed": "ts-node src/database/seed.ts",
    "reset-admin": "ts-node src/database/reset-admin-password.ts",
    "check-admin": "ts-node src/database/check-admin.ts",
    "check-users": "ts-node src/database/check-users.ts",
    "check-clubs": "ts-node src/database/check-clubs.ts",
    "check-isvalidated": "ts-node src/database/check-isvalidated.ts",
    "backup": "ts-node src/database/backup.ts",
    "test-supabase": "node test-supabase-connection.js",
    "diagnose-supabase": "node diagnose-supabase-connection.js",
    "export-data": "ts-node src/database/export-data.ts",
    "import-data": "ts-node src/database/import-data.ts",
    "check-data": "ts-node src/database/check-data.ts",
    "add-payment-status-cancelled": "ts-node src/database/add-payment-status-cancelled.ts",
    "add-vessel-id-to-cashes": "ts-node src/database/add-vessel-id-to-cashes.ts",
    "create-income-tables": "ts-node src/database/create-income-tables.ts",
    "add-category-id-to-cash-transactions": "ts-node src/database/add-category-id-to-cash-transactions.ts"
  },
  "keywords": [
    "crm",
    "marina",
    "yacht-club",
    "booking"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "postgres": "^3.4.7",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
